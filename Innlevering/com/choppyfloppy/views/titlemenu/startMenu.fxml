<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0" type="javafx.scene.layout.VBox" stylesheets="@title.css" alignment="CENTER">

    <Label styleClass="titleMenuStart" text="TITTEL TIL SPILL" />

    <Button fx:id="startButton" styleClass="menubutton" mnemonicParsing="false" text="Start" onAction="#startClicked" />
    <Button fx:id="fortsettButton" styleClass="menubutton" mnemonicParsing="false" text="Continue" onAction="#continueClicked"/>
    <Button fx:id="exitButton" styleClass="menubutton" mnemonicParsing="false" text="Exit" onAction="#exitClicked"/>

</fx:root>
